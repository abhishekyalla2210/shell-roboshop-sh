
app_setup(){
     id roboshop &>>$LOG_FILE
     if [ $? -ne 0]; then
    useradd --system --home /app --shell /sbin/nologin --comment "roboshop system user" roboshop &>>$LOG_FILE
    VALIDATE $? "Creating system user"
    else
    echo -e "User already exists ... $Y SKIPPING $N" | tee -a $LOG_FILE
    fi
}